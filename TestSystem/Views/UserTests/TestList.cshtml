@model IEnumerable<TestSystem.ViewModels.UserTests.TestListViewModel>

@Styles.Render("~/Content/UserTestList.css")

<section class="item-container">
    @foreach (var userTest in Model)
    {
    <div class="user-test">
        <h1>@userTest.Name</h1>
        <a href='@Url.Action("Test", "UserTests", new { userTestId = userTest.UserTestId }, null)'>start</a>
    </div>
    }
</section>